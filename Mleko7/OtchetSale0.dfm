  object quOtchet: TMSQuery
    Connection = dmDataModule.DB
    SQL.Strings = (
      
        'SELECT     Post.Name + '#39' ('#39' + AddressPost.Address + '#39')'#39' AS Name,' +
        ' SUM(Rashod.Kol) AS Kol,  SUM(Rashod.PriceOut * Rashod.Kol) AS S' +
        'umm, Tovar.TovarNo'
      'FROM         Rashod INNER JOIN'
      
        '                      NaklR ON Rashod.NaklNo = NaklR.NaklNo INNE' +
        'R JOIN'
      
        '                      Post ON NaklR.PostNoFirst = Post.PostNo IN' +
        'NER JOIN'
      
        '                      AddressPost ON NaklR.PostNoFirst = Address' +
        'Post.PostNo AND NaklR.AddressNoFirst = AddressPost.AddressNo INN' +
        'ER JOIN'
      '                      Tovar ON Rashod.TovarNo = Tovar.TovarNo'
      'WHERE     (Tovar.VidNo IN (:VidNo)) AND (Rashod.Kol > 0) '
      
        '                   AND (NaklR.DateNaklFirst >= :DateBegin) AND (' +
        'NaklR.DateNaklFirst <= :DateEnd)'
      
        'GROUP BY Post.Name, Post.NameLong, AddressPost.Address, Tovar.To' +
        'varNo'
      'ORDER BY Post.Name, AddressPost.Address')
    Left = 72
    Top = 24
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'VidNo'
      end
      item
        DataType = ftUnknown
        Name = 'DateBegin'
      end
      item
        DataType = ftUnknown
        Name = 'DateEnd'
      end>
    object quOtchetName: TStringField
      FieldName = 'Name'
      FixedChar = True
      Size = 83
    end
    object quOtchetKol: TFloatField
      FieldName = 'Kol'
    end
    object quOtchetSumm: TFloatField
      FieldName = 'Summ'
      DisplayFormat = '0.00'
      EditFormat = '0.00'
      currency = True
    end
    object quOtchetTovarNo: TSmallintField
      FieldName = 'TovarNo'
    end
  end
