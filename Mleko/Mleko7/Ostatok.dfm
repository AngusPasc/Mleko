  object quOstatok: TMSQuery
    Connection = dmDataModule.DB
    SQL.Strings = (
      'SELECT   o.tovarno'
      '         ,o.kol-isnull(res.qty,0) as kol'
      '         ,isnull(res.qty,0) as res_qty'
      '         ,o.kol as kol_all'
      '         ,o.price'
      '         ,o.kolhold'
      '         ,t.nametovar'
      '         ,t.kolperpak'
      '         ,o.price1'
      '         ,o.price2'
      '         ,o.percent1'
      '         ,o.percent2'
      '         ,o.lastpricein'
      '         ,o.avgpricein'
      '         ,v.vidname'
      '         ,o.price3'
      '         ,o.price4'
      '         ,o.price5'
      '         ,o.price6'
      '         ,o.price7'
      '         ,o.price8'
      '         ,o.price9'
      '         ,o.price13'
      '         ,o.price14'
      '         ,o.price15'
      '         ,pfv.PriceVeb'
      '         ,pfv.PriceInInst'
      '         ,t.weight'
      '         ,pfv.Currency'
      '         ,(select Rate'
      '            from d_currency dc left join'
      
        '                 CurrencyExchange ce on ce.CurrencyId = dc.ID an' +
        'd isActive = 1'
      '             where l_code = pfv.Currency) as Rate'
      'FROM     ostatok o INNER JOIN '
      '         tovar t ON o.tovarno = t.TovarNo LEFT OUTER JOIN '
      '         vidtov v ON v.vidno = t.vidno left outer join '
      
        '         (select s.article_id,sum(s.qty) as qty from DSPEC s whe' +
        're s.STATE_ID=4 group by s.ARTICLE_ID) res on o.TovarNo=res.ARTI' +
        'CLE_ID left join '
      '         PriceForVeb pfv on pfv.TovarNo = o.TovarNo'
      '         where &_where  &_flt'
      'ORDER BY v.vidname'
      '         ,nametovar')
    Filter = 'vidname = '#39' '#39
    Left = 208
    Top = 206
    MacroData = <
      item
        Name = '_where'
        Value = '1=1'
      end
      item
        Name = '_flt'
      end>
  end
