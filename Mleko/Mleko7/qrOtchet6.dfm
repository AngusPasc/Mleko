  object quRashod: TMSQuery
    Connection = dmDataModule.DB
    SQL.Strings = (
      'select Rashod.TovarNo,Sum(Kol*PriceOut) SummaOut,Sum(Kol) Kol,'
      
        '       cast(0.00 as float) PriceIn,Tovar.NameTovar,Company.NameC' +
        'ompany,'
      '       cast('#39' '#39' as VarChar(5)) TipPrice'
      
        'from Rashod,NaklR,Tovar left outer join Company on (Company.Comp' +
        'anyNo=Tovar.CompanyNo)'
      'where  (Tovar.TovarNo=Rashod.TovarNo) and'
      '       (NaklR.NaklNo=Rashod.NaklNo)   and'
      '       (NaklR.DateNaklFirst>=:DateBegin)   and'
      '       (NaklR.DateNaklFirst<=:DateEnd)     and'
      
        '       (NaklR.PostNo<>108) and (NaklR.PostNo<>114) and (NaklR.Po' +
        'stNo<>385)'
      'group by Company.NameCompany,Tovar.NameTovar,Rashod.TovarNo'
      'order by Company.NameCompany,Tovar.NameTovar'
      '')
    CachedUpdates = True
    UpdateObject = UpdateSQL1
    OnCalcFields = quRashodCalcFields
    Left = 16
    Top = 16
    ParamData = <
      item
        DataType = ftDate
        Name = 'DateBegin'
      end
      item
        DataType = ftDate
        Name = 'DateEnd'
      end>
    object quRashodNameTovar: TStringField
      FieldName = 'NameTovar'
      Size = 30
