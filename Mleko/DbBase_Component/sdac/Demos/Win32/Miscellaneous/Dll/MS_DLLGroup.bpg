#------------------------------------------------------------------------------
VERSION	= BWS.01
#------------------------------------------------------------------------------
!ifndef	ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = MS_DLL.dll MS_Exe.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

MS_Exe.exe: MS_Exe.dpr
  $(DCC)

MS_DLL.dll: MS_DLL.dpr
  $(DCC)


