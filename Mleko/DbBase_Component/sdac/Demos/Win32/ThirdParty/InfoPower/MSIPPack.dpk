// This demo was built and tested using InfoPower 2000 Trial for Delphi 5.
// You can recompile it under other versions by changing requires section manually.

package MSIPPack;

{$R *.RES}
{$ALIGN ON}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION OFF}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'SDAC InfoPower compatible components'}
{$DESIGNONLY}
{$IMPLICITBUILD ON}

requires
  vcl50,
  vcldb50,
  sdac50,
  ip50d_d5trial;

contains
  MSIP in 'MSIP.pas';

end.
